=begin%>
 <%= form_with(model: @author, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.input :email_field :email %>

  <%= f.label :name>
  <%= f.input :text_field :name %>

  <%= f.label :dob>
  <%= f.input :date_field :dob %>>

  <%= f.label :password%>
  <%= f.input :password_field :password %>

  <%= f.submit "Sign Up" %>
<%end%> 
